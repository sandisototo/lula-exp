{
  "name": "lula-app",
  "version": "1.0.0",
  "description": "lula-app",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=local --o --host 0.0.0.0",
    "start:dev": "ng serve --configuration=development --o --host 0.0.0.0",
    "start:prod": "ng serve --configuration=production --o --host 0.0.0.0",
    "build:local": "ng build --configuration=local",
    "build:dev": "ng build --prod --configuration=development",
    "build:prod": "ng build --prod",
    "pwa:build:local": "ng build --configuration=local && sw-precache --root=dist --config=precache-config.js",
    "pwa:build:dev": "ng build --prod --configuration=development && sw-precache --root=dist --config=precache-config.js",
    "pwa:build:prod": "ng build --prod && sw-precache --root=dist --config=precache-config.js",
    "test": "ng test",
    "e2e": "ng e2e",
    "generate": "ng generate",
    "compodoc:gen": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "compodoc:serve": "./node_modules/.bin/compodoc -s src/tsconfig.app.json",
    "compodoc": "npm run compodoc:gen && npm run compodoc:serve",
    "precommit": "lint-staged",
    "lint": "tslint --fix --config tslint.json --project tsconfig.json",
    "format:fix": "pretty-quick --staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "pretty-quick --staged",
      "tslint --fix --config tslint.json --project tsconfig.json",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandisototo/lula-exp.git"
  },
  "keywords": [
    "angular",
    "angular6",
    "angularjs",
    "starter-kit",
    "lula-app",
    "angular.io"
  ],
  "author": "Mayank Patel <patelmayankce@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/patelmayankce/angular-7-boilerplate/issues"
  },
  "homepage": "https://github.com/patelmayankce/angular-7-boilerplate#readme",
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@angular/service-worker": "^7.2.15",
    "@asymmetrik/ngx-leaflet": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^3.1.0",
    "agm-direction": "^0.7.8",
    "angular-font-awesome": "^3.1.2",
    "angular-notifier": "^4.1.1",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.9",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.0",
    "leaflet": "^1.5.1",
    "leaflet-routing-machine": "^3.2.12",
    "luxon": "^1.15.0",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.4",
    "@angular/cli": "^7.0.7",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/jquery": "^3.3.29",
    "@types/leaflet-routing-machine": "^3.2.1",
    "@types/luxon": "^1.15.1",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "protractor": "^5.4.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "3.1.6"
  }
}
